class Solution(object):
    def longestConsecutive(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if not nums:
            return 0
        val=1
        vals=1
        nums=sorted(list(set(nums)))
        for i in range(len(nums)-1):

            if nums[i]==nums[i+1]-1:
                vals+=1
            else:
                vals=1
            val=max(val,vals)
        return val

        

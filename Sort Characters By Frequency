class Solution(object):
    def frequencySort(self, s):
        """
        :type s: str
        :rtype: str
        """
        char_frequency = Counter(s)
      
       
        sorted_chars = sorted(char_frequency.items(), key=lambda item: item[1],reverse=True)
      
        
        result = ''.join(char * freq for char, freq in sorted_chars)
      
        return result
        

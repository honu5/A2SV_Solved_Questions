class Solution(object):
    def findValidPair(self, s):
        """
        :type s: str
        :rtype: str
        """
        count=Counter(s)
        t=""
        for i in range(len(s)-1):
            if s[i]!=s[i+1] and count[s[i]]==int(s[i]) and count[s[i+1]]==int(s[i+1]):
                return s[i:i+2]
        return t
        
